<div id="searchResults">
  <h3>Search results</h3>
  <div class="table-responsive">
    <table class="table table-hover">
      <thead>
        <th></th>
        <th>Title</th>
        <th>Brand</th>
        <th>Price</th>
        <th>Stock</th>
      </thead>
      <tbody>
        <% for(var i =0;i<info.length;i++) {%>
          <tr class="searchItem">
            <td><img src=<%=info[i].image %> alt="" class="thumbnail"></td>
            <td><%= info[i].title %></td>
            <td><%= info[i].brand %></td>
            <td><%= info[i].price %></td>
            <td><%= info[i].stock %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</div>
